#!/usr/bin/env python3
import mcp
from mcp.types import CallToolResult, TextContent
import inspect

print(f"MCP version: {mcp.__version__}")
print(f"CallToolResult signature: {inspect.signature(CallToolResult)}")
print(f"TextContent signature: {inspect.signature(TextContent)}")

# Test creating a CallToolResult
try:
    result = CallToolResult(content=[TextContent(type="text", text="test")])
    print("✅ CallToolResult created successfully")
except Exception as e:
    print(f"❌ Error creating CallToolResult: {e}")

# Test alternative creation methods
try:
    result = CallToolResult(content=[TextContent(text="test")])
    print("✅ CallToolResult created successfully (without type)")
except Exception as e:
    print(f"❌ Error creating CallToolResult (without type): {e}")